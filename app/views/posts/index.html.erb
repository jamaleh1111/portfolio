<h1 id="page_title">Blog</h1>
<div id="posts_wrapper" class="skinny_wrapper">
  <div class="post">
    <% @posts.each do |post| %>
    <h3><a href="#"><%= link_to @post.title, post %></a></h3>
    <p class="date"><%= @post.create_at.strftime("%A, %b, %d") %></p>
    <% end %>
    <hr>
  </div>

</div>

  <div class="pagination">
    <span class="previous_page disabled">Previous</span>
    <em class="current">1</em>
    <a rel="next" href="#">2</a>
    <a class="next_page" href="#">Next</a>
  </div>
